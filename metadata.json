{
  "name": "ocadotechnology-puppet",
  "version": "2.0.0",
  "summary": "Resource types for managing puppet master and agent",
  "license": "APSL-2.0",
  "source": "git://github.com/ocadotechnology/puppetmodule.git",
  "project_page": "https://github.com/ocadotechnology/puppetmodule",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": "5.x"
    }
  ],
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">=3.0.0"},
    {"name":"puppetlabs/apt","version_requirement":">=1.1.0"},
    {"name":"puppetlabs/inifile","version_requirement":">=1.0.0"}
  ]
}
